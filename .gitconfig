[core]
	quotepath = false
	whitespace = cr-at-eol
	autoCRLF = false
[alias]
	ss = status
	br = branch
	brm = branch -m
	brd = branch -d
	brdd = branch -D
	co = checkout
	cob = checkout -b
	adu = add -u
	adup = add -u -p
	com = commit
	mg = merge --no-ff
	mgff = merge -ff
	cp = cherry-pick
	log1 = log -1
	log2 = log -2
	log3 = log -3
	logo = log --oneline
	logn = log --name-status --oneline